{
  "require": {
    "bronskiycraftsolpaceexpressformstelegram/craft-solspace-express-forms-telegram-notifications": "^1.0",
    "carlcs/craft-redactorcustomstyles": "3.0.3",
    "craftcms/cms": "3.4.22.1",
    "craftcms/contact-form": "2.2.7",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/element-api": "2.6.0",
    "craftcms/redactor": "2.6.1",
    "solspace/craft-express-forms": "1.0.5",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "./craft-plugins/craftsolspaceexpressformstelegramnotifications"
    }
  ]
}
